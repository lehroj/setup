[alias]
  a    = add
  all  = add -A .
  br   = branch
  brd  = branch -D
  c    = commit
  cm   = commit -m
  ca   = commit --amend
  cam  = commit --amend -m
  cl   = clone
  co   = checkout
  cob  = checkout -b
  d    = diff
  m    = merge
  pl   = pull
  ps   = push
  psf  = push --force
  rb   = rebase
  rba  = rebase --abort
  rbc  = rebase --continue
  rbs  = rebase --skip
  r    = remote
  re   = reset
  st   = status
  sts  = status -s
  ss   = stash
  ssu  = stash -u