[alias]
  a = add
  aa = add -A .
  br = branch
  brd = branch -D
  c = commit
  cm = commit -m
  ca = commit --amend
  cam = commit --amend -m
  cl = clone
  co = checkout
  cob = checkout -b
  d = diff
  ds = diff --stat
  dc = diff --cached
  la = "!git config -l | grep alias | cut -c 7-"
  lg = log --oneline --graph --decorate --all
  lga = log --oneline --graph --decorate --all --color
  m = merge
  pl = pull
  ps = push
  psf = push --force
  rb = rebase
  rba = rebase --abort
  rbc = rebase --continue
  rbs = rebase --skip
  r  = remote
  re = reset
  st = status
  sts = status -s
  ss = stash
  ssu = stash -u
